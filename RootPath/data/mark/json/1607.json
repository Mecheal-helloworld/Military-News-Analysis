[{"sentence": "\u4e2d\u4e1c\u5c40\u52bf\u7d27\u5f20\u4e4b\u9645\u00a0\u7f8e\u56fd\u5411\u7ea6\u65e6\u90e8\u7f72\u7231\u56fd\u80053\u5bfc\u5f39", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u3010\u73af\u7403\u7f51\u519b\u4e8b\u62a5\u9053\u3011", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u636e\u7f8e\u56fd\u201c\u9632\u52a1\u535a\u5ba2\u201d\u7f51\u7ad91\u67086\u65e5\u62a5\u9053\uff0c\u5728\u4e2d\u4e1c\u5c40\u52bf\u9aa4\u7136\u7d27\u5f20\u4e4b\u9645\uff0c\u7f8e\u56fd\u7a7a\u519b\u5411\u8be5\u5730\u533a\u7d27\u6025\u589e\u6d3e\u4e86\u7231\u56fd\u8005-3\u9632\u7a7a\u5bfc\u5f39\u7cfb\u7edf\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u62a5\u9053\u79f0\uff0c\u7f8e\u56fd\u8bb0\u8005\u5df4\u5df4\u514b.\u5854\u683c\u74e6\u4f0a\u5728\u793e\u4ea4\u5a92\u4f53\u4e0a\u8bf4\uff1a\u201c\u7f8e\u56fd\u7a7a\u519b\u6b63\u5728\u901a\u8fc7C-5\u8fd0\u8f93\u673a\u5c06\u5305\u62ec\u7231\u56fd\u8005-3\u9632\u7a7a\u53cd\u5bfc\u7cfb\u7edf\u5728\u5185\u7684\u91cd\u578b\u88c5\u5907\u90e8\u7f72\u81f3\u7ea6\u65e6\uff0c\u4ee5\u9632\u6b62\u7f8e\u519b\u57fa\u5730\u548c\u8bbe\u65bd\u906d\u5230\u4f0a\u6717\u4f0a\u65af\u5170\u9769\u547d\u536b\u961f\u7684\u5bfc\u5f39\u6253\u51fb\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u201d", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u8fd9\u4e5f\u662f\u7f8e\u56fd\u603b\u7edf\u7279\u6717\u666e\u53c8\u4e0b\u4ee4\u72d9\u6740\u4f0a\u6717\u5c06\u519b\u82cf\u83b1\u66fc\u5c3c\u7684\u884c\u52a8\u540e\uff0c\u7f8e\u56fd\u5728\u4e2d\u4e1c\u7684\u6700\u65b0\u9632\u536b\u90e8\u7f72\u52a8\u4f5c\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u5728\u521a\u521a\u8fc7\u53bb\u7684\u5468\u672b\uff0c\u4f0a\u6717\u9ad8\u7ea7\u519b\u5b98\u82cf\u83b1\u66fc\u5c3c\u5728\u7f8e\u519b\u7a7a\u88ad\u4e2d\u8eab\u4ea1\uff0c\u7f8e\u4f0a\u7d27\u5f20\u5c40\u52bf\u8fdb\u4e00\u6b65\u52a0\u5267\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u6b64\u524d\u636eCNN\u7f51\u7ad9\u62a5\u9053\uff0c\u5728\u8fd9\u4e00\u80cc\u666f\u4e0b\uff0c\u7f8e\u56fd\u62df\u5411\u4e2d\u4e1c\u5730\u533a\u589e\u6d3e\u7ea63000\u540d\u58eb\u5175\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u636eCNN\u4ecb\u7ecd\uff0c\u8fd9\u4e9b\u58eb\u5175\u5927\u591a\u6765\u81ea\u5317\u5361\u7f57\u6765\u7eb3\u5dde\u8d39\u8036\u7279\u7ef4\u5c14\u5e02(Fayetteville)\uff0c\u4e00\u5ea7\u4e34\u8fd1\u5e03\u62c9\u683c\u5821\u7f8e\u519b\u57fa\u5730\u7684\u57ce\u5e02\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}]