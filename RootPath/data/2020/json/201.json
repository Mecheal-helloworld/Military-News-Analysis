[{"sentence": "\u4fc4\u519b\u5c06\u7c73\u683c31\u6218\u673a\u90e8\u7f72\u5230\u7f8e\u56fd\u201c\u5bb6\u95e8\u53e3\u201d\u00a020\u5206\u949f\u5c31\u80fd\u98de\u62b5\u963f\u62c9\u65af\u52a0", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u3010\u73af\u7403\u7f51\u62a5\u9053 \u8bb0\u8005 \u5f90\u7490\u660e\u3011", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u4fc4\u7f57\u65af\u8fd1\u65e5\u5c06\u7c73\u683c-31\u622a\u51fb\u673a\u90e8\u7f72\u5230\u4e86\u7f8e\u56fd\u963f\u62c9\u65af\u52a0\u5730\u533a\u9644\u8fd1\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u201c\u9632\u52a1\u535a\u5ba2\u201d\u7f51\u7ad912\u67083\u65e5\u62a5\u9053\uff0c\u4fc4\u7f57\u65af\u592a\u5e73\u6d0b\u8230\u961f\u65b0\u95fb\u5904\u53d1\u5e03\u58f0\u660e\u79f0\uff0c\u8be5\u8230\u961f\u5728\u5176\u4f4d\u4e8e\u4e1c\u5317\u90e8\u592a\u5e73\u6d0b\u6cbf\u5cb8\u7684\u6e2f\u53e3\u3001\u9a6c\u52a0\u4e39\u5dde\u695a\u79d1\u5947\u81ea\u6cbb\u533a\u9996\u5e9c\u963f\u7eb3\u5fb7\u5c14\u7684\u57fa\u5730\u90e8\u7f72\u4e86\u7c73\u683c-31\u622a\u51fb\u673a\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u636e\u6089\uff0c\u963f\u7eb3\u5fb7\u5c14\u4e0e\u7f8e\u56fd\u963f\u62c9\u65af\u52a0\u5730\u533a\u6700\u8fd1\u8ddd\u79bb\u4ec5600\u5343\u7c73,\u98de\u884c\u65f6\u95f4\u4ec5\u970020\u5206\u949f\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u62a5\u9053\u79f0\uff0c\u4e3a\u4e86\u52a0\u5f3a\u5317\u6781\u5730\u533a\u7684\u822a\u7a7a\u4f5c\u6218\u529b\u91cf\uff0c\u4fc4\u7f57\u65af\u6d77\u519b\u822a\u7a7a\u5175\u7684\u7c73\u683c-31\u88ab\u90e8\u7f72\u5230\u4e86\u963f\u7eb3\u5fb7\u5c14\u5730\u533a\uff0c\u6267\u884c\u9632\u7a7a\u6218\u6597\u4efb\u52a1\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u963f\u7eb3\u5fb7\u5c14\u6240\u5728\u7684\u695a\u79d1\u5947\u81ea\u6cbb\u533a\u9760\u8fd1\u7f8e\u56fd\u963f\u62c9\u65af\u57fa\u5730\u533a\uff0c\u6240\u4ee5\u8be5\u5730\u76f8\u5f53\u654f\u611f\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u7c73\u683c-31\u662f\u4e00\u6b3e\u53cc\u5ea7\u578b\u5168\u5929\u5019\u8d85\u97f3\u901f\u8fdc\u7a0b\u622a\u51fb\u673a\uff0c\u6700\u65b0\u578b\u7684\u7c73\u683c-31BM\u53ef\u4ee5\u5728\u666e\u901a\u6218\u6597\u673a\u7684\u5c04\u7a0b\u5916\u5bf9\u5176\u8fdb\u884c\u8fdc\u8ddd\u79bb\u7784\u51c6\u62e6\u622a\uff0c\u914d\u5907\u4e86R-33\u8fdc\u7a0b\u7a7a\u7a7a\u5bfc\u5f39\u548cR-73\u8fd1\u7a0b\u7a7a\u7a7a\u5bfc\u5f39\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}, {"sentence": "\u7c73\u683c-31BM\u6218\u6597\u673a\u7684\u6700\u5927\u8d77\u98de\u91cd\u91cf\u8fbe46.2\u5428\uff0c\u914d\u59074\u679aR-33\u5bfc\u5f39\u548c\u526f\u6cb9\u7bb1\u65f6\u822a\u7a0b\u80fd\u8fbe\u52303000\u516c\u91cc\u3002", "events": [], "arguments": {"loc": [], "time": [], "country": [], "weapon": []}}]